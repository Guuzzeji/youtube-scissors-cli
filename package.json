{
  "name": "youtube-scissors-cli",
  "version": "1.0.0",
  "description": "A simple cli-app that allows you to divide a YouTube video into multiple separate videos base on a video's time stamps. Power by pkg and @Guuzzeji yt-scissors npm library",
  "main": "index.js",
  "files": [
    "fetch",
    "ffmpeg-control"
  ],
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "youtube",
    "download",
    "video",
    "ffmpeg",
    "cli",
    "video-processing"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Guuzzeji/youtube-scissors-cli"
  },
  "author": "Guuzzeji <gabe.business.code@outlook.com> (https://github.com/Guuzzeji)",
  "license": "MIT",
  "dependencies": {
    "@freetube/yt-comment-scraper": "^6.1.0",
    "chalk": "^5.0.1",
    "figlet": "^1.5.2",
    "prompts": "^2.4.2",
    "signale": "^1.4.0",
    "ytdl-core": "^4.11.1"
  },
  "scripts": {
    "start": "node index.js",
    "build": "pkg . --compress"
  },
  "bin": "index.js",
  "pkg": {
    "assets": "node_modules/**/*",
    "targets": [
      "node14-win-x64",
      "node14-linux-x64",
      "node14-macos-x64"
    ],
    "outputPath": "build"
  }
}
